[% META
	otitle = 'Ämnen som avslagits'
	level = 5
%]

[% PROCESS load_member.tt %]

[% order_default = 't';
   direction_default = 'desc';
   PROCESS pagify.tt;
%]

<h1>Ämnen som avslagits: [% m.tlink %]</h1>

[% PROCESS person_view_menu.tt %]


<div class="entry">
<table>

<tr><th>[% sort('tid','t') %]
    <th>[% sort('Titel', 't_title') %]
    <th>[% sort('Skapad', 't_created') %]
    <th>Ändrad av
</tr>

[% FOREACH rec = select_list("from t where t_createdby=? and t_changedby<>? and t_status = 0 and t_entry is false order by $order $direction offset ? limit ?", m.member, m.member, part_offset, [], pagesize, []) %]
   [% t = topic(rec.t, rec.t_ver) %]

   [%tr2%]
        <td align="right">[% t.id %]</td>
        <td[% UNLESS t.active_ver %] style="text-decoration: line-through"[% END %]>[% jump(t.desig, '/member/db/topic/view/', tid=t.id) %] v[% t.ver %] [%+ PROCESS typelist.tt rel=t.id %]</td>
	<td>[% t.created.ymd %]</td>
	<td>
        [% IF t.active %]
           oförändrad
        [% ELSIF t.active_ver %]
           ersatt med v[% t.active_ver.ver %]
        [% ELSIF t.replaced_by %]
           Ersatt med [% jump(t.replaced_by.id, '/member/db/topic/view/', tid=t.replaced_by.id) %]
        [% ELSE %]
           inaktiv
        [% END %]</td>
   </tr>
   [% subpart.shown = loop.size %]
[% END %]

</table>
</div>

[% PROCESS pagify_footer.tt %]
